{
    "name": "stream-fairy",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.1.3",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/styled-components": "^5.1.26",
        "axios": "^1.4.0",
        "bootstrap": "^5.2.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.9",
        "react-router-dom": "^6.11.1",
        "react-scripts": "5.0.1",
        "styled-components": "^6.0.0-rc.1",
        "typescript": "*"
    },
    "devDependencies": {
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.59.5",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.32.2",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.2",
        "prettier": "^2.8.8"
    },
    "scripts": {
        "dev": "BROWSER=none react-scripts start",
        "dev:build": "react-scripts build",
        "dev:test": "react-scripts test",
        "dev:eject": "react-scripts eject",
        "dev:check": "prettier  --check .",
        "dev:lint": "eslint src/**/*.{js,jsx,ts,tsx,json}  --max-warnings=0",
        "dev:Fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
        "dev:format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "dev:prepare": "husky install",
        "prepare": "husky install"
    },
    "husky": {
        "hook": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx": [
            "yarn dev:lint"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
